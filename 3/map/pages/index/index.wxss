/**index.wxss**/
.footer{
    width:100%;
    height:86rpx;
    position: fixed;
    display: flex;
    justify-content: space-between;
    left: 0;
    bottom: 0;
    z-index: 10;
}
.footer>view{
    width:50%;
    height:100%;
    line-height: 86rpx;
    text-align: center;
    color: #fff
}
.footer>view:nth-child(1){
    background: #ff9700
}
.footer>view:nth-child(2){
    background: #0bb0c3
}
.container{
    position: fixed;
    left:0;
    top:0;
    right:0;
    bottom:86rpx;
    z-index: 5
}
.container>map{
    width:100%;
    height:100%;
}