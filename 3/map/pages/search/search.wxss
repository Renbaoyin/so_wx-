.container{
    height:100%;
    width:100%;
}
.top{
    width:100%;
    height:98rpx;
    background: #03bbd5;
    position: relative
}

.top>input{
    width:590rpx;
    height:58rpx;
    background: white;
    border-radius: 10px;
    position: absolute;
    top:10px;
    left:5px;
    text-align: center

    
}
.top>view{
    width:116rpx;
    height:58rpx;
    background:#03bbd5 ;
    position: absolute;
    top:10px;
    right:0
}
.center{
    width:100%;
    height:100;
    background: #f5f5f5
}
.result{
    width:100%;
    height:200rpx;
    margin-bottom: 20px;
    background: white   
}

.result>view:nth-child(1){
    width:100%;
    height:60rpx;
    line-height: 60rpx;
    color: #cfa551;
    margin-bottom: 10px;
}
.result>view:nth-child(2){
    margin-top: 31px;
    width:100%;
    height:80rpx;
    line-height: 80rpx;
    color: #5d5d5d
}
